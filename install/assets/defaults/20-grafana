#!/command/with-contenv bash

ANONYMOUS_HIDE_VERSION=${ANONYMOUS_HIDE_VERSION:-"TRUE"}
ANONYMOUS_ORG_NAME=${ANONYMOUS_ORG_NAME:-"Main Org."}
ANONYMOUS_ROLE=${ANONYMOUS_ROLE:-"Viewer"}
AUTO_ASSIGN_ORG_ID=${AUTO_ASSIGN_ORG_ID:-"1"}
AUTO_ASSIGN_ORG_ROLE=${AUTO_ASSIGN_ORG_ROLE:-"Viewer"}
CONFIG_FILE=${CONFIG_FILE:-"config.ini"}
CONFIG_PATH=${CONFIG_PATH:-"/config/"}
COOKIE_NAME=${COOKIE_NAME:-"grafana_session"}
DATA_PATH=${DATA_PATH:-"/data/"}
DEFAULT_THEME=${DEFAULT_THEME:-"dark"}
DISABLE_LOGIN_FORM=${DISABLE_LOGIN_FORM:-"FALSE"}
DISABLE_SIGNOUT=${DISABLE_SIGNOUT:-"FALSE"}
ENABLE_ANALYTICS=${ENABLE_ANALYTICS:-"FALSE"}
ENABLE_ANONYMOUS=${ENABLE_ANONYMOUS:-"FALSE"}
ENABLE_AUTO_ASSIGN_ORG=${ENABLE_AUTO_ASSIGN_ORG:-"TRUE"}
ENABLE_OAUTH=${ENABLE_OAUTH:-"FALSE"}
ENABLE_ORG_CREATE=${ENABLE_ORG_CREATE:-"TRUE"}
ENABLE_SIGN_UP=${ENABLE_SIGN_UP:-"TRUE"}
ENABLE_SIGV4_AUTH=${ENABLE_SIGV4_AUTH:-"FALSE"}
HOME_PATH=${HOME_PATH:-"/usr/share/grafana/"}
LIFETIME_API_KEY_MAXIMUM=${LIFETIME_API_KEY_MAXIMUM:-"-1"}
LIFETIME_LOGIN_MAXIMUM=${LIFETIME_LOGIN_MAXIMUM:-"30d"}
LIFETIME_LOGIN_MAXIMUM_INACTIVE=${LIFETIME_LOGIN_MAXIMUM_INACTIVE:-"7d"}
LIFETIME_OAUTH_STATE_COOKIE=${LIFETIME_OAUTH_STATE_COOKIE:-"600"}
LISTEN_PORT=${LISTEN_PORT:-"3000"}
LISTEN_PROTOCOL=${LISTEN_PROTOCOL:-"http"}
LOGIN_HINT=${LOGIN_HINT:="email or username"}
LOG_FORMAT=${LOG_FORMAT:-"text"}
LOG_FORMAT_CONSOLE=${LOG_FORMAT_CONSOLE:-"${LOG_FORMAT}"}
LOG_FORMAT_FILE=${LOG_FORMAT_FILE:-"${LOG_FORMAT}"}
LOG_LEVEL=${LOG_LEVEL:-"info"}
LOG_LEVEL_CONSOLE=${LOG_LEVEL_CONSOLE:-"${LOG_LEVEL}"}
LOG_LEVEL_FILE=${LOG_LEVEL_FILE:-"${LOG_LEVEL}"}
LOG_PATH=${LOG_PATH:-"/logs/"}
LOG_TYPE=${LOG_TYPE:-"console"}
LOG_WEB_REQUESTS=${LOG_WEB_REQUESTS:-"TRUE"}
MAIL_FROM=${MAIL_FROM:-"Grafana"}
OAUTH_ALLOW_SIGNUP=${OAUTH_ALLOW_SIGNUP:-"TRUE"}
OAUTH_API_URL=${OAUTH_API_URL:-"https://auth.example.com/oauth2/userinfo"}
OAUTH_ATTRIBUTE_EMAIL=${OAUTH_ATTRIBUTE_EMAIL:-"email"}
OAUTH_AUTH_URL=${OAUTH_AUTH_URL:-"https://auth.example.com/oauth2/authorize"}
OAUTH_AUTO_LOGIN=${OAUTH_AUTO_LOGIN:-"TRUE"}
OAUTH_CLIENT_ID=${OAUTH_CLIENT_ID:-"grafana"}
OAUTH_CLIENT_SECRET=${OAUTH_CLIENT_SECRET:-"secret"}
OAUTH_PROVIDER_NAME=${OAUTH_PROVIDER_NAME:-"SSO"}
OAUTH_SCOPES=${OAUTH_SCOPES:-"openid email profile"}
OAUTH_SEND_CLIENT_CREDS_POST=${OAUTH_SEND_CLIENT_CREDS_POST:-"FALSE"}
OAUTH_TOKEN_URL=${OAUTH_TOKEN_URL:-"https://auth.example.com/oauth2/token"}
PASSWORD_HINT=${PASSWORD_HINT:="password"}
PLUGINS_PATH=${PLUGINS_PATH:-"${DATA_PATH}/plugins"}
PLUGIN_CATALOG_URL=${PLUGIN_CATALOG_URL:-"https://grafana.com/grafana/plugins/"}
PLUGIN_ENABLE_ADMIN_EXERNAL_MANAGE=${PLUGIN_ENABLE_ADMIN_EXERNAL_MANAGE:-"false"}
PLUGIN_ENABLE_ALPHA=${PLUGIN_ENABLE_ALPHA:-"FALSE"}
PLUGIN_ENABLE_INSTALL_ADMIN=${PLUGIN_ENABLE_INSTALL_ADMIN:-"TRUE"}
PLUGIN_SKIP_TLS_VERIFY=${PLUGIN_SKIP_TLS_VERIFY:-"FALSE"}
PROVISIONING_PATH=${PROVISIONING_PATH:-"${CONFIG_PATH}/provisioning"}
SETUP_TYPE=${SETUP_TYPE:-"AUTO"}
SIGNOUT_REDIRECT_URL=${SIGNOUT_REDIRECT_URL:-""}
SIGNUP_VERIFY_EMAIL=${SIGNUP_VERIFY_EMAIL:-"false"}
DB_TYPE=${DB_TYPE:-"sqlite"}
DB_HOST=${DB_HOST:-"localhost"}
DB_PORT=${DB_PORT:-"3306"}
DB_NAME=${DB_NAME:-"grafana"}
DB_USER=${DB_USER:-"grafana"}
DB_PASS=${DB_PASS:-"password"}
